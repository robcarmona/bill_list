<div class="col-xs-12">
  <div class="row" ng-controller="sessionController">
    <div class="col-xs-12">
      <div class="row">
        <div class="col-xs-6">
          <h3> Bill List {{ page.title }} </h3>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4 col-md-offset-4">
          <form ng-submit="loginSubmit()" role="form">
            <label for="email">Email</label>
            <input type="email" class="form-control" ng-model="user.email" name="text" id="email"/>
            <label for="password">Password</label>
            <input type="password" class="form-control" ng-model="user.password" name="text" id="password" />
            <input type="submit" class="btn btn-default signin-btn" id="submit" value="{{ page.title }}" />
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
